<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"  ng-app="ticketApp">
<head>
    <title>@ViewBag.Title</title>
   

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE8" />
    <title>Infoweb Services Support Desk</title>
    <meta name="KEYWORDS" content="Home" />
    <!-- <meta name="description" content="DISPLAY SOME KB STUFF OR NEWS HERE" /> -->
    <meta name="robots" content="index,follow" />
    <link rel="shortcut icon" href="/favicon.ico" />

    <link href="/StyleSheet/JuerColorPicker.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" type="text/css" href="/StyleSheet/helpdesk.css" />

    <script src="/Scripts/jquery-1.8.2.min.js" type="text/javascript"></script>
     <script src="/Scripts/angular.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.10/angular-route.js"></script>
    <script src="~/Scripts/SPA.js"></script>
     @Scripts.Render("~/bundles/angular" + HttpContext.Current.Cache["JSVersion"])
</head>
<body class="bodymain">

   

    <div id="main">
        <div id="topbanner">
            <!-- Custom topbanner code -->
            <table width="100%">
                <tr>
                    <td align="left">
                        <a href="index.aspx">
                            <img border="0" src="/images/InfowebServiceLogo.gif"
                                 alt="InfoWebServices Fusion Logo" id="logo" />
                        </a>
                    </td>
                    <td align="right"></td>
                </tr>
            </table>
        </div>
        <!-- Custom topbanner code end -->
        <div id="toptoolbar">
            <span id="toptoolbarrightarea">
                <select class="swiftselect" name="languageid" id="languageid" onchange="javascript: LanguageSwitch(false);">
                    <option value="1" selected=selected>English (U.S.)</option>
                    <%--
                    <option value="2">Chinese (C.N.)</option>
                    <option value="4">Türkçe</option>
                    <option value="5">??????? (Russian)</option>
                    <option value="7">Español</option>--%>
                </select>
            </span>
            <ul id="toptoolbarlinklist">
                <li>

                    <a href="#"  style = "background-image: url(data:image/png;base64,)",class = "toptoolbarlink" >Home</a>

                </li>
                @if (User.Identity.IsAuthenticated)
                {
                 
                       <li class="current">
      

                                        @Html.ActionLink("View Tickets", "TicketList", "Ticket", null,
                                               new
                                               {
                                                   style = "background-image: url(data:image/png;base64,)",
                                                   @class = "toptoolbarlink"
                                               })
                    
                    </li>
                }

                <li>
                    
                       <a href="#submit_ticket"  style = "background-image: url(data:image/png;base64,)",class = "toptoolbarlink" >Submit a Ticket</a>


                </li>
            </ul>
        </div>
        <div id="maincore" ng-controller="mainController">
            <div>{{rightPanelUrl}}</div>
             <div  ng-include src="rightPanelUrl">
        </div>
          
              <div  ng-view>

                  </div>
            <div>
                  @RenderBody()
            </div>

            <div id="bottomfooter" class="bottomfooterpadding">
            </div>
        </div>
  
 
</body>
</html>